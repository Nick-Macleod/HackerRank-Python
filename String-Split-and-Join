def split_and_join(line):
    # write your code here
    
    #split acts on a string, have to use space delimiter " "
    
    split_line=line.split(" ")
    
    #join acts on the joining delimeter ("-"), argument is the thing to join
    
    joined_line="-".join(split_line)
    return(joined_line)

if __name__ == '__main__':
    line = input()
    result = split_and_join(line)
    print(result)
